{
  "name": "slack-stylize",
  "author": "DeanKH",
  "description": "expand the selected emoji icon in slack emoji preview window.",
  "version": "1.0.2",
  "manifest_version": 2,
  "web_accessible_resources": ["*"],
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://app.slack.com/client/*"],
      "css": ["slack/style.css"]
    }
  ]
}
